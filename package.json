{
  "name": "electron-quick-start-2",
  "version": "1.0.0",
  "description": "A minimal Electron application",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "dev": "nodemon ./bin/www",
    "pack": "cd .. && electron-packager ./electron-express --ignore=\"^(?!.(node_modules|dist)).(src|screenshots|tsconfig|README|.git)\" --asar.unpack=\".*\\.(node|dll|db)$\" --overwrite"
  },
  "repository": "https://github.com/electron/electron-quick-start",
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial",
    "demo"
  ],
  "author": "GitHub",
  "license": "CC0-1.0",
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "ejs": "~3.1.10",
    "electron-packager": "^17.1.2",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "morgan": "~1.9.1"
  },
  "devDependencies": {
    "electron": "^32.1.0",
    "nodemon": "^3.1.4"
  }
}
